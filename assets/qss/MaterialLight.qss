QMainWindow {
	background-color:#f4f2fa;
}
QDialog {
	background-color:#c8c5d9;
}
QColorDialog {
	background-color:#c8c5d9;
}
QTextEdit {
	background-color:#c8c5d9;
	color: #474c52;
}
QPlainTextEdit {
	selection-background-color:#007b50;
	background-color:#c8c5d9;
	border-radius: 10px;
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 1px;
	color: #474c52;
}
QPushButton{
	border-style: solid;
	border-radius: 10px;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 3px;
	border-style: solid;
	color: #474c52;
	padding: 2px;
	background-color: #c8c5d9;
}
QPushButton::default{
	border-style: inset;
	border-radius: 10px;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #0d4e87;
	border-width: 3px;
	color: #474c52;
	padding: 2px;
	background-color: #c8c5d9;
}
QToolButton {
	border-style: solid;
	border-top-color: transparent;
	border-radius: 10px;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #0d4e87;
	border-bottom-width: 1px;
	border-style: solid;
	color: #474c52;
	padding: 2px;
	background-color: #c8c5d9;
}
QToolButton:hover{
	border-style: solid;
	border-top-color: transparent;
	border-radius: 10px;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #0d4e87;
	border-bottom-width: 2px;
	border-style: solid;
	color: #FFFFFF;
	padding-bottom: 1px;
	background-color: #c8c5d9;
}
QPushButton:hover{
	border-style: solid;
	border-top-color: transparent;
	border-radius: 10px;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #0d4e87;
	border-bottom-width: 3px;
	border-style: solid;
	color: #FFFFFF;
	padding-bottom: 2px;
	background-color: #c8c5d9;
}
QPushButton:pressed{
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-radius: 10px;
	border-bottom-color: #0d4e87;
	border-width: 5px;
	border-style: solid;
	color: #0d4e87;
	padding-bottom: 1px;
	background-color: #c8c5d9;
}
QPushButton:disabled{
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-radius: 10px;
	border-left-color: transparent;
	border-bottom-color: #808086;
	border-bottom-width: 2px;
	border-style: solid;
	color: #808086;
	padding-bottom: 1px;
	background-color: #c8c5d9;
}
QLineEdit {
	border-width: 1px; border-radius: 4px;
	border-color: rgb(58, 58, 58);
	border-radius: 10px;
	border-style: inset;
	padding: 0 8px;
	color: #474c52;
	background:#c8c5d9;
	selection-background-color:#007b50;
	selection-color: #FFFFFF;
}
QLabel {
	color: #474c52;
	border-radius: 10px;
}
QLCDNumber {
	color: #37e6b4;
	border-radius: 10px;
}
QProgressBar {
	text-align: center;
	color: rgb(240, 240, 240);
	border-width: 1px; 
	border-radius: 10px;
	border-radius: 10px;
	border-color: rgb(58, 58, 58);
	border-style: inset;
	background-color:#c8c5d9;
}
QProgressBar::chunk {
	background-color: #0d4e87;
	border-radius: 5px;
}
QMenuBar {
	border-radius: 10px;
	background-color: #c8c5d9;
}
QMenuBar::item {
	color: #474c52;
  	spacing: 3px;
	border-radius: 10px;
  	padding: 1px 4px;
  	background: #c8c5d9;
}

QMenuBar::item:selected {
  	background:#c8c5d9;
	color: #FFFFFF;
}
QMenu::item:selected {
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: #0d4e87;
	border-bottom-color: transparent;
	border-left-width: 2px;
	color: #FFFFFF;
	padding-left:15px;
	padding-top:4px;
	padding-bottom:4px;
	padding-right:7px;
	background-color: #c8c5d9;
}
QMenu::item {
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-bottom-width: 1px;
	border-style: solid;
	color: #474c52;
	padding-left:17px;
	padding-top:4px;
	padding-bottom:4px;
	padding-right:7px;
	background-color: #c8c5d9;
}
QMenu{
	background-color:#c8c5d9;
	border-radius: 10px;
}
QTabWidget {
	color:rgb(0,0,0);
	background-color:#c8c5d9;
}
QTabWidget::pane {
		border-color: rgb(77,77,77);
		background-color:#c8c5d9;
		border-style: solid;
	border-radius: 10px;
		border-width: 1px;
    	border-radius: 6px;
}
QTabBar::tab {
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-bottom-width: 1px;
	border-style: solid;
	color: #808086;
	padding: 3px;
	margin-left:3px;
	background-color: #c8c5d9;
}
QTabBar::tab:selected, QTabBar::tab:last:selected, QTabBar::tab:hover {
  	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #0d4e87;
	border-bottom-width: 2px;
	border-style: solid;
	color: #FFFFFF;
	padding-left: 3px;
	padding-bottom: 2px;
	margin-left:3px;
	background-color: #c8c5d9;
}

QCheckBox {
	color: #474c52;
	padding: 2px;
	border-radius: 10px;
}
QCheckBox:disabled {
	color: #808086;
	padding: 2px;
	border-radius: 10px;
}

QCheckBox:hover {
	border-radius:4px;
	border-style:solid;
	border-radius: 10px;
	padding-left: 1px;
	padding-right: 1px;
	padding-bottom: 1px;
	padding-top: 1px;
	border-width:1px;
	border-color: rgb(87, 97, 106);
	background-color:#c8c5d9;
}
QCheckBox::indicator:checked {

	height: 10px;
	width: 10px;
	border-radius: 10px;
	border-style:solid;
	border-width: 1px;
	border-color: #0d4e87;
	color: #474c52;
	background-color: #0d4e87;
}
QCheckBox::indicator:unchecked {

	height: 10px;
	width: 10px;
	border-style:solid;
	border-radius: 10px;
	border-width: 1px;
	border-color: #0d4e87;
	color: #474c52;
	background-color: transparent;
}
QRadioButton {
	color: #474c52;
	background-color: #c8c5d9;
	border-radius: 10px;
	padding: 1px;
}
QRadioButton::indicator:checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-radius:5px;
	border-width: 1px;
	border-color: #0d4e87;
	color: #474c52;
	background-color: #0d4e87;
}
QRadioButton::indicator:!checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-radius:5px;
	border-width: 1px;
	border-color: #0d4e87;
	color: #474c52;
	background-color: transparent;
}
QStatusBar {
	color:#027f7f;
}
QSpinBox {
	color: #474c52;	
	background-color: #c8c5d9;
}
QDoubleSpinBox {
	color: #474c52;	
	background-color: #c8c5d9;
}
QTimeEdit {
	color: #474c52;	
	background-color: #c8c5d9;
}
QDateTimeEdit {
	color: #474c52;	
	background-color: #c8c5d9;
}
QDateEdit {
	color: #474c52;	
	background-color: #c8c5d9;
}
QComboBox {
	color: #474c52;	
	background: #c8c5d9;
	border-radius: 10px;
}
QComboBox:editable {
	background: #c8c5d9;
	color: #474c52;
	selection-background-color: #c8c5d9;
}
QComboBox QAbstractItemView {
	color: #474c52;	
	background: #c8c5d9;
	selection-color: #FFFFFF;
	selection-background-color: #c8c5d9;
}
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	color: #474c52;	
	background: #c8c5d9;
}
QFontComboBox {
	color: #474c52;	
	background-color: #c8c5d9;
	border-radius: 10px;
}
QToolBox {
	color: #474c52;
	background-color: #c8c5d9;
}
QToolBox::tab {
	color: #474c52;
	background-color: #c8c5d9;
}
QToolBox::tab:selected {
	color: #FFFFFF;
	background-color: #c8c5d9;
}
QScrollArea {
	color: #FFFFFF;
	background-color: #c8c5d9;
}
QSlider::groove:horizontal {
	height: 5px;
	background: #0d4e87;
}
QSlider::groove:vertical {
	width: 5px;
	background: #0d4e87;
}
QSlider::handle:horizontal {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
	border: 1px solid #5c5c5c;
	width: 14px;
	margin: -5px 0;
	border-radius: 7px;
}
QSlider::handle:vertical {
	background: qlineargradient(x1:1, y1:1, x2:0, y2:0, stop:0 #b4b4b4, stop:1 #8f8f8f);
	border: 1px solid #5c5c5c;
	height: 14px;
	margin: 0 -5px;
	border-radius: 7px;
}
QSlider::add-page:horizontal {
    background: white;
}
QSlider::add-page:vertical {
    background: white;
}
QSlider::sub-page:horizontal {
    background: #0d4e87;
}
QSlider::sub-page:vertical {
    background: #0d4e87;
}
QListView {
    show-decoration-selected: 1;
	color: #a9b7c6;
	background-color:#f4f2fa;
    border-radius: 10px;
}

QListView::item:alternate {
    background: #0d4e87;
}

QListView::item:selected {
    border: 1px solid #0d4e87;
    border-radius: 4px;
}

QListView::item:selected:!active {
    background: #0d4e87;
    border-radius: 4px;
}

QListView::item:selected:active {
    background: #0d4e87;
    border-radius: 4px;
}

QListView::item:hover {
    border-radius: 4px;
    background: #163147;
}



